<% layout('layouts/boilerplate')%>
<div id="map" class="map" style="width: 100%; height:50vh"></div>
    <h1>ww</h1>
    <a href="/campground/new">Add campground</a>
        <%for (let camps of camp){%>
            <div class="card mb-3">
                <div class="row">
                    <div class="col-md-4">
                     <% if(camps.images.length) { %>
                        <img src="<%= camps.images[0].url %>" alt="" class="img-fluid rounded-start">
                    <% }else{%> 
                        <img src="https://images.unsplash.com/photo-1491199967891-34b84fb35af4?crop=entropy&cs=tinysrgb&fit=max&fm=jpg&ixid=MnwyOTQ3MTB8MHwxfGNvbGxlY3Rpb258MXwwUFM5M3MzQ2NGNHx8fHx8Mnx8MTY0MzE4MDQyMw&ixlib=rb-1.2.1&q=80&w=1080" alt="" class="img-fluid rounded-start"> 
                    <% } %>    
                        
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h4 class="card-title"><%= camps.title %></h4>
                            <p class="card-text"><%= camps.description %></p>
                            <p class="card-text">
                                <small class="text-muted"><%= camps.location %></small>
                            </p>
                            <p class="card-text"><strong>$<%= camps.price %></strong>/Night</p>
                            <a href="/campground/<%= camps._id %>" class="btn btn-primary">View Details</a>
                        </div>
                    </div>
                </div>
            </div>
       <% } %>
<script>
    const mapToken = '<%-process.env.MAPBOX_TOKEN%>';
    const camp = {features: <%- JSON.stringify(camp) %>}
</script>
<script src="clusterMap.js"></script>
